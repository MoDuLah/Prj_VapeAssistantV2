                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>

                                <TextBlock Grid.Column="0" Grid.Row="1" Text="1." TextAlignment="Right" x:Name="sn1" Margin="0,0,2,0"/>
                                <TextBox Grid.Column="1" Grid.Row="1" VerticalContentAlignment="Center" x:Name="product1" LostFocus="emptyTextBoxColoring" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                <TextBox Grid.Column="2" Grid.Row="1" GotFocus="Dash_GotFocus" LostFocus="Dash_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" x:Name="sku1" Text="-" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" BorderBrush="{x:Null}"/>
                                <TextBox Grid.Column="3" Grid.Row="1" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="Quantity_GotFocus" LostFocus="Quantity_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" x:Name="quantity1" Text="0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" BorderBrush="{x:Null}"/>
                                <TextBox Grid.Column="4" Grid.Row="1" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" x:Name="price1"    Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6" BorderBrush="{x:Null}"/>
                                <TextBox Grid.Column="5" Grid.Row="1" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" x:Name="discount1" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6" BorderBrush="{x:Null}"/>
                                <TextBox Grid.Column="6" Grid.Row="1" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" x:Name="shipping1" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6" BorderBrush="{x:Null}"/>
