                <GroupBox Grid.Row="2" Grid.Column="2" Header="{x:Static p:Resources.purchases_Items}" Margin="4,10,4,4">
                <TextBlock x:Name="additems_Count" Text="1" TextAlignment="Center" />
                </GroupBox>
                <GroupBox Grid.Row="2" Grid.Column="3" Header="{x:Static p:Resources.purchases_TotalCost}" Margin="4,10,4,4">
                    <TextBlock x:Name="order_sum" Text="1" TextAlignment="Center" />
                </GroupBox>


