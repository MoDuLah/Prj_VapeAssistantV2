<UserControl x:Name="usrc_purchases" x:Class="Vape_Assistant.Views.Purchases_ChildView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:p="clr-namespace:Vape_Assistant.Properties"
             xmlns:local="clr-namespace:Vape_Assistant.Views"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:myCalendar="clr-namespace:Vape_Assistant"
             Foreground="{Binding Source={x:Static p:Settings.Default}, Path=FGColor, Mode=TwoWay}"
             FontFamily="{Binding Source={x:Static p:Settings.Default}, Path=FontFam, Mode=TwoWay}"
             mc:Ignorable="d"
             FontSize="20">
    <!--Initialized="usrc_purchases_Initialized"-->
    <TabControl Background="{x:Null}" BorderBrush="{x:Null}">
        <TabControl.Resources>
            <Style TargetType="TabItem">
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="TabItem">
                            <Border Name="Border" BorderThickness="1,1,1,0" BorderBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColor, Mode=TwoWay}" CornerRadius="4,4,0,0" Margin="50,5,5,0" Padding="10,5,10,0">
                                <ContentPresenter x:Name="ContentSite"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        ContentSource="Header"
                                        Margin="10,0"/>
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="IsSelected" Value="True">
                                    <Setter TargetName="Border" Property="Background" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" />
                                </Trigger>
                                <Trigger Property="IsSelected" Value="False">
                                    <Setter TargetName="Border" Property="Background" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColor, Mode=TwoWay}" />
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
        </TabControl.Resources>
        <TabItem Padding="20,0" IsTabStop="False">
            <TabItem.Header>
                <StackPanel Orientation="Vertical">
                    <Image Source="/Vape Assistant;component/Images/Plus_32px.png" VerticalAlignment="Center" Width="32"/>
                    <TextBlock Text="{x:Static p:Resources.purchases_tabitem1}" VerticalAlignment="Center"/>
                </StackPanel>
            </TabItem.Header>
            <Border Background="{Binding Source={x:Static p:Settings.Default}, Path=innerBGColor, Mode=TwoWay}" BorderThickness="5,2,5,2" CornerRadius="30" Margin="10,0" Padding="10,0" >
                <Border.BorderBrush>
                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#FFB90000" Offset="0"/>
                        <GradientStop Color="#FF680000" Offset="1"/>
                    </LinearGradientBrush>
                </Border.BorderBrush>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>

                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition Height="Auto"/>
                    </Grid.RowDefinitions>
                    <TextBlock Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" x:Name="purchase_date_lbl"  Text="{x:Static p:Resources.purchase_DateOfPurchase}" Padding="2" Margin="20,0" HorizontalAlignment="Center" VerticalAlignment="Bottom"/>
                    <StackPanel Grid.Row="0" Grid.Column="3" Orientation="Horizontal" HorizontalAlignment="Center">
                        <TextBlock x:Name="purchase_vendor_lbl" Grid.Row="0" Grid.Column="3" Text="{x:Static p:Resources.purchase_VendorName}" VerticalAlignment="Bottom" HorizontalAlignment="Center"/>
                        <Button x:Name="Add_V" Grid.Row="0" Grid.Column="3" Click="Add_V_Click" AutomationProperties.AcceleratorKey="A" MaxWidth="205" MaxHeight="40">
                            <Button.Style>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="Background" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColor, Mode=TwoWay}"/>
                                    <Setter Property="Foreground" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonFGColor, Mode=TwoWay}"/>
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type Button}">
                                                <Border Background="{TemplateBinding Background}" CornerRadius="0,0,0,0" Padding="5" Margin="10,5">
                                                    <ContentPresenter HorizontalAlignment="Right" VerticalAlignment="Center"/>
                                                </Border>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Background" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                            <Setter Property="Foreground" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonFGColorHover, Mode=TwoWay}"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Button.Style>
                            <StackPanel Orientation="Horizontal">
                                <Image Source="/Vape Assistant;component/Images/Plus_32px.png" Width="18" HorizontalAlignment="Right" RenderTransformOrigin="0.5,0.55"/>
                                <TextBlock Text="{x:Static p:Resources.AddShop}"/>
                            </StackPanel>
                        </Button>
                    </StackPanel>
                    <TextBlock Grid.Row="0" Grid.Column="4" x:Name="purchase_ord_num_lbl" Text="{x:Static p:Resources.purchase_OrderId}" HorizontalAlignment="Center" Padding="2" Margin="20,0" VerticalAlignment="Bottom"/>
                    <myCalendar:ucCalendar Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" x:Name="purchase_date" Width="Auto" TabIndex="0" HorizontalAlignment="Center" IsTabStop="True"/>
                    <ComboBox Grid.Row="1" Grid.Column="3" MinWidth="120" x:Name="purchase_vendor_add" TabIndex="1" Margin="5,0" VerticalContentAlignment="Center" HorizontalContentAlignment="Stretch"/>
                    <TextBox Grid.Row="1" Grid.Column="4" x:Name="purchase_ord_num" AllowDrop="False" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" TabIndex="2"/>

                    <StackPanel Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="5" x:Name="ItemsHeader" Margin="0,10,0,0" >
                        <Grid HorizontalAlignment="Center">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="35"/>
                                <ColumnDefinition Width="120" ToolTip="{x:Static p:Resources.purchase_Product}"/>
                                <ColumnDefinition Width="120" ToolTip="{x:Static p:Resources.purchase_SKU}"/>
                                <ColumnDefinition Width="100" ToolTip="{x:Static p:Resources.purchase_Quantity}"/>
                                <ColumnDefinition Width="100" ToolTip="{x:Static p:Resources.purchase_Price}"/>
                                <ColumnDefinition Width="100" ToolTip="{x:Static p:Resources.purchase_Discount}"/>
                                <ColumnDefinition Width="100" ToolTip="{x:Static p:Resources.purchase_Shipping}"/>
                                <ColumnDefinition Width="100" ToolTip="{x:Static p:Resources.purchase_Total}" />
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <StackPanel Orientation="Horizontal" Grid.Column="1" Grid.Row="0" HorizontalAlignment="Center" Width="120" MinWidth="120" MaxWidth="120">
                                <TextBlock Text="{x:Static p:Resources.purchase_Product}" TextAlignment="Center" Margin="20,0,0,0" />
                                <TextBlock Text="*" TextAlignment="Right" Margin="0,0,20,0" Foreground="DarkRed"/>
                            </StackPanel>
                            <TextBlock Grid.Column="2" Grid.Row="0" Width="120" MinWidth="120" MaxWidth="120" Text="{x:Static p:Resources.purchase_SKU}" TextAlignment="Center" Margin="-5,0,0,0" />
                            <StackPanel Orientation="Horizontal" Grid.Column="3" Grid.Row="0" HorizontalAlignment="Center" Width="120" MinWidth="120" MaxWidth="120">
                                <TextBlock Text="{x:Static p:Resources.purchase_Quantity}" TextAlignment="Center" Margin="20,0,0,0" />
                                <TextBlock Text="*" TextAlignment="Right" Margin="0,0,20,0" Foreground="DarkRed"/>
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Grid.Column="4" Grid.Row="0" HorizontalAlignment="Center" Width="100" MinWidth="100" MaxWidth="100">
                                <TextBlock Text="{x:Static p:Resources.purchase_Price}" TextAlignment="Center" Margin="20,0,0,0"/>
                                <TextBlock Text="*" TextAlignment="Right" Margin="0,0,20,0" Foreground="DarkRed"/>
                            </StackPanel>
                            <TextBlock Grid.Column="5" Grid.Row="0" Width="100" MinWidth="100" MaxWidth="100" Text="{x:Static p:Resources.purchase_Discount}" TextAlignment="Center" />
                            <TextBlock Grid.Column="6" Grid.Row="0" Width="100" MinWidth="100" MaxWidth="100" Text="{x:Static p:Resources.purchase_Shipping}" TextAlignment="Center" />
                            <TextBlock Grid.Column="7" Grid.Row="0" Width="100" MinWidth="100" MaxWidth="100" Text="{x:Static p:Resources.purchase_Total}" TextAlignment="Center"/>
                            <ScrollViewer x:Name="ProductScroller" Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="8" Height="Auto" MinHeight="250" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Auto" HorizontalAlignment="Center" MaxHeight="250" Margin="0,0" Padding="0,0,2,0">
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="35"/>
                                        <ColumnDefinition Width="120" ToolTip="{x:Static p:Resources.purchase_Product}"/>
                                        <ColumnDefinition Width="120" ToolTip="{x:Static p:Resources.purchase_SKU}"/>
                                        <ColumnDefinition Width="100" ToolTip="{x:Static p:Resources.purchase_Quantity}"/>
                                        <ColumnDefinition Width="100" ToolTip="{x:Static p:Resources.purchase_Price}"/>
                                        <ColumnDefinition Width="100" ToolTip="{x:Static p:Resources.purchase_Discount}"/>
                                        <ColumnDefinition Width="100" ToolTip="{x:Static p:Resources.purchase_Shipping}"/>
                                        <ColumnDefinition Width="100" ToolTip="{x:Static p:Resources.purchase_Total}" />
                                    </Grid.ColumnDefinitions>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="Auto"/>
                                    </Grid.RowDefinitions>

                                    <TextBlock Grid.Column="0" Grid.Row="1" Text="1." TextAlignment="Right" x:Name="sn1" Margin="0,0,2,0"/>
                                    <TextBox Grid.Column="1" Grid.Row="1" VerticalContentAlignment="Center" Width="120" MinWidth="120" MaxWidth="120" x:Name="product1" LostFocus="emptyTextBoxColoring" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="2" Grid.Row="1" GotFocus="Dash_GotFocus" LostFocus="Dash_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="120" MinWidth="120" MaxWidth="120" x:Name="sku1" Text="-" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" BorderBrush="{x:Null}"/>
                                    <TextBox Grid.Column="3" Grid.Row="1" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="Quantity_GotFocus" LostFocus="Quantity_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="quantity1" Text="0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" BorderBrush="{x:Null}"/>
                                    <TextBox Grid.Column="4" Grid.Row="1" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="price1"    Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6" BorderBrush="{x:Null}"/>
                                    <TextBox Grid.Column="5" Grid.Row="1" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="discount1" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6" BorderBrush="{x:Null}"/>
                                    <TextBox Grid.Column="6" Grid.Row="1" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="shipping1" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6" BorderBrush="{x:Null}"/>
                                    <TextBox Grid.Column="7" Grid.Row="1" TextChanged="Change_Total_Cost" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="total1"    Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6" BorderBrush="{x:Null}"/>

                                    <TextBlock Grid.Column="0" Grid.Row="2" Text="2." TextAlignment="Right" x:Name="sn2" Margin="0,0,2,0"/>
                                    <TextBox Grid.Column="1" Grid.Row="2" VerticalContentAlignment="Center" Width="120" MinWidth="120" MaxWidth="120" x:Name="product2" LostFocus="emptyTextBoxColoring" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="2" Grid.Row="2" GotFocus="Dash_GotFocus" LostFocus="Dash_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="120" MinWidth="120" MaxWidth="120" x:Name="sku2"      Text="-" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="3" Grid.Row="2" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="Quantity_GotFocus" LostFocus="Quantity_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="quantity2" Text="0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="4" Grid.Row="2" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="price2"    Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>
                                    <TextBox Grid.Column="5" Grid.Row="2" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="discount2" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>
                                    <TextBox Grid.Column="6" Grid.Row="2" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="shipping2" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>
                                    <TextBox Grid.Column="7" Grid.Row="2" TextChanged="Change_Total_Cost" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="total2"    Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>

                                    <TextBlock Grid.Column="0" Grid.Row="3" Text="3." TextAlignment="Right" x:Name="sn3" Margin="0,0,2,0"/>
                                    <TextBox Grid.Column="1" Grid.Row="3" VerticalContentAlignment="Center" Width="120" MinWidth="120" MaxWidth="120" x:Name="product3" LostFocus="emptyTextBoxColoring" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="2" Grid.Row="3" GotFocus="Dash_GotFocus" LostFocus="Dash_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="120" MinWidth="120" MaxWidth="120" x:Name="sku3"      Text="-" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="3" Grid.Row="3" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="Quantity_GotFocus" LostFocus="Quantity_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="quantity3" Text="0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="4" Grid.Row="3" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="price3"    Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>
                                    <TextBox Grid.Column="5" Grid.Row="3" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="discount3" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>
                                    <TextBox Grid.Column="6" Grid.Row="3" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="shipping3" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>
                                    <TextBox Grid.Column="7" Grid.Row="3" TextChanged="Change_Total_Cost" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="total3"    Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>

                                    <TextBlock Grid.Column="0" Grid.Row="4" Text="4." TextAlignment="Right" x:Name="sn4" Margin="0,0,2,0"/>
                                    <TextBox Grid.Column="1" Grid.Row="4" VerticalContentAlignment="Center" Width="120" MinWidth="120" MaxWidth="120" x:Name="product4" LostFocus="emptyTextBoxColoring" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="2" Grid.Row="4" GotFocus="Dash_GotFocus" LostFocus="Dash_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="120" MinWidth="120" MaxWidth="120" x:Name="sku4" Text="-" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="3" Grid.Row="4" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="Quantity_GotFocus" LostFocus="Quantity_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="quantity4" Text="0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="4" Grid.Row="4" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="price4"   Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>
                                    <TextBox Grid.Column="5" Grid.Row="4" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="discount4" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>
                                    <TextBox Grid.Column="6" Grid.Row="4" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="shipping4" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>
                                    <TextBox Grid.Column="7" Grid.Row="4" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="total4" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>

                                    <TextBlock Grid.Column="0" Grid.Row="5" Text="5." TextAlignment="Right" x:Name="sn5" Margin="0,0,2,0"/>
                                    <TextBox Grid.Column="1" Grid.Row="5" VerticalContentAlignment="Center" Width="120" MinWidth="120" MaxWidth="120" x:Name="product5" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="2" Grid.Row="5" GotFocus="Dash_GotFocus" LostFocus="Dash_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="120" MinWidth="120" MaxWidth="120" x:Name="sku5"      Text="-" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="3" Grid.Row="5" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="Quantity_GotFocus" LostFocus="Quantity_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="quantity5" Text="0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                    <TextBox Grid.Column="4" Grid.Row="5" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="price5"    Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>
                                    <TextBox Grid.Column="5" Grid.Row="5" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="discount5" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>
                                    <TextBox Grid.Column="6" Grid.Row="5" TextChanged="Change_Total" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="shipping5" Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>
                                    <TextBox Grid.Column="7" Grid.Row="5" TextChanged="Change_Total_Cost" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Width="100" MinWidth="100" MaxWidth="100" x:Name="total5"    Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>

                                    <TextBlock Grid.Column="0" Grid.Row="6" Text="6." TextAlignment="Right" x:Name="sn6" Margin="0,0,2,0"/>
