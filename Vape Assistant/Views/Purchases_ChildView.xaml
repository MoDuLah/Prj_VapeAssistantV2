                <Button x:Name="purchase_submit" Grid.Column="4" Grid.Row="3" Content="{x:Static p:Resources.Save_Button}" Click="purchase_submit_Click" IsDefault="True" VerticalAlignment="Top" Margin="10,5" />

                <TextBox x:Name="purchase_error_catch" Visibility="Hidden" IsTabStop="False"/>
                <TextBlock Grid.Row="1" Grid.Column="6" x:Name="retrievedID" MinWidth="40" Visibility="Hidden" HorizontalAlignment="Right"/>
                <TextBlock Grid.Row="1" Grid.Column="7" x:Name="retrievedOrders" MinWidth="40" Visibility="Hidden"/>
            </Grid>
        </TabItem>
        <TabItem Padding="20,0" IsTabStop="False" >
            <TabItem.Header>
                <StackPanel Orientation="Vertical" Margin="10,0">
                    <Image Source="/Vape Assistant;component/Images/menuIconsLight/Edit File_32px.png" VerticalAlignment="Center" Width="32"/>
                    <TextBlock Text="{x:Static p:Resources.purchases_tabitem2}" VerticalAlignment="Center"/>
                </StackPanel>
            </TabItem.Header>
            <Grid HorizontalAlignment="Center" VerticalAlignment="Top">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>

                <GroupBox Grid.Row="0" Grid.Column="0" Header="{x:Static p:Resources.purchase_ViewSince}" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="4,5,4,0">
                    <myCalendar:ucCalendar Grid.Row="1" Grid.Column="0" x:Name="datepSince" Width="Auto" TabIndex="0" HorizontalAlignment="Center"/>
                </GroupBox>
                <GroupBox Grid.Row="0" Grid.Column="1" Header="{x:Static p:Resources.purchase_ViewUntil}" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="4,5,4,0">
                    <myCalendar:ucCalendar Grid.Row="1" Grid.Column="1" x:Name="datepUntil" Width="Auto" TabIndex="1" />
                </GroupBox>
                <GroupBox Grid.Row="2" Grid.Column="0" Margin="4,4,4,0" Header="{x:Static p:Resources.purchase_ViewVendor}" HorizontalAlignment="Center" VerticalAlignment="Bottom" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" >
                    <StackPanel Orientation="Horizontal">
                        <ComboBox  MinWidth="200" MaxWidth="200" x:Name="purchase_vendor_view" TabIndex="2" LostFocus="purchase_vendor_view_LostFocus" VerticalAlignment="Center" Margin="5,0,0,0" />
                        <Button x:Name="Load_table_txt" Content="{x:Static p:Resources.vendor_Load}" Click="Load_table" HorizontalAlignment="Center" FontSize="12"/>
                    </StackPanel>
                </GroupBox>
                <GroupBox Grid.Row="2" Grid.Column="1" Margin="4,4,4,0" Header="{x:Static p:Resources.vendor_Reference}" HorizontalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" VerticalAlignment="Bottom">
                    <StackPanel Orientation="Horizontal">
                        <ComboBox  MinWidth="200" MaxWidth="200" x:Name="purchase_reference" TabIndex="2" VerticalAlignment="Center" Margin="5,0,0,0" />
                        <Button x:Name="Load_by_reference" Content="{x:Static p:Resources.vendor_Load}" Click="Load_tabled" HorizontalAlignment="Center" FontSize="12"/>
                    </StackPanel>
                </GroupBox>
                <GroupBox Grid.Row="2" Grid.Column="2" Grid.RowSpan="2" Header="{x:Static p:Resources.purchase_Selection}" VerticalAlignment="Bottom" Margin="4,4,4,0">
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                        <Button x:Name="EntryEdit" Content="{x:Static p:Resources.vendor_edit}" Click="Button_Edit_Element_Click" FontSize="12"/>
                        <Button x:Name="EntryDelete" Content="{x:Static p:Resources.vendor_delete}" Click="EntryDelete_Click" FontSize="12"/>
                    </StackPanel>
                </GroupBox>
                <GroupBox Grid.Row="2" Grid.Column="3" x:Name="orderslbl" Header="{x:Static p:Resources.vendor_TotalOrders}" VerticalAlignment="Center" Margin="4,1,4,0">
                    <TextBox x:Name="Orders" MinWidth="20" Width="Auto" Text="0" IsTabStop="False" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" FontSize="20"/>
                </GroupBox>
                <GroupBox Grid.Row="2" Grid.Column="4" x:Name="orderstotlbl" Header="{x:Static p:Resources.vendor_SumOrders}"  VerticalAlignment="Bottom" Margin="4,4,4,0">
                    <TextBox x:Name="sumOrders" MinWidth="40" Width="Auto" Text="0" IsTabStop="False" IsEnabled="False" FontSize="20" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                </GroupBox>
                <DataGrid Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="5" 
                          x:Name="ViewEntries" Height="300" MinHeight="100" MaxHeight="400"
                          Width="Auto" MinWidth="600"
                          Background="#FF242424"
                          AlternatingRowBackground="#FF424242"  AlternationCount="-1" AutoGenerateColumns="False"
                          FontSize="16" Foreground="White" HorizontalGridLinesBrush="#FFB00101" VerticalGridLinesBrush="#FFB00101" 
                          Margin="0,5,0,0" IsReadOnly="True"  VerticalAlignment="Top"
                          TabIndex="4" SelectionChanged="ViewEntries_SelectionChanged"
                          SelectedItem="{Binding txtbox, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" BorderThickness="2">
                    <DataGrid.BorderBrush>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FF464646" Offset="0"/>
                            <GradientStop Color="#FF707070" Offset="1"/>
                        </LinearGradientBrush>
                    </DataGrid.BorderBrush>
                    <DataGrid.Columns>
                        <DataGridTextColumn x:Name="IdColumn" Binding="{Binding Id}" Header=" # " Width="Auto"/>
                        <DataGridTextColumn x:Name="DateColumn" Binding="{Binding Date}" Header=" Order Date " Width="Auto" MaxWidth="200" Foreground="Black"/>
                        <DataGridTextColumn x:Name="ReferenceColumn" Binding="{Binding Reference}" Header="Order Ref." MaxWidth="200" Width="Auto"/>
                        <DataGridTextColumn x:Name="NameColumn" Binding="{Binding Name}" Header=" Company " MaxWidth="200" Width="Auto"/>
                        <DataGridTextColumn x:Name="VIDColumn" Binding="{Binding VID}" Header=" Company ID " MaxWidth="200" Width="Auto"/>
                        <DataGridTextColumn x:Name="DescriptionColumn" Binding="{Binding Description}" Header=" Product Desc. " Width="Auto"/>
                        <DataGridTextColumn x:Name="PIDColumn" Binding="{Binding PID}" Header=" Product Id (SKU) " MaxWidth="200" Width="Auto"/>
                        <DataGridTextColumn x:Name="QuantityColumn" Binding="{Binding Quantity}" Header=" Quantity " MaxWidth="200" Width="Auto"/>
                        <DataGridTextColumn x:Name="PriceColumn" Binding="{Binding Price}" Header=" Price ($) " MaxWidth="200" Width="Auto"/>
                        <DataGridTextColumn x:Name="DiscountColumn" Binding="{Binding Discount}" Header=" Discount ($) " MaxWidth="200" Width="Auto"/>
                        <DataGridTextColumn x:Name="ShippingColumn" Binding="{Binding Shipping}" Header=" Shipping ($) " MaxWidth="200" Width="Auto"/>
                        <DataGridTextColumn x:Name="TotalColumn" Binding="{Binding Total}" Header=" Total Cost ($) " MaxWidth="200" Width="Auto"/>
                    </DataGrid.Columns>
                </DataGrid>
                <Grid Grid.Row="6" Grid.RowSpan="1" Grid.ColumnSpan="5" x:Name="EditElementInputBox" Visibility="Collapsed">
