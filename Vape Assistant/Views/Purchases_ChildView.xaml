                                        </Setter.Value>
                                    </Setter>
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Background" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                            <!-- -->
                                            <Setter Property="Foreground" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonFGColorHover, Mode=TwoWay}"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Button.Style>
                        </Button>
                    </StackPanel>
                    <StackPanel Grid.Row="2" Grid.Column="2"  Grid.RowSpan="2" Margin="0,10,0,0" VerticalAlignment="Bottom">
                        <TextBlock Grid.Row="4" Grid.Column="2" Text="{x:Static p:Resources.purchase_Selection}" HorizontalAlignment="Center" VerticalAlignment="Bottom" FontSize="20"/>
                        <StackPanel Grid.Row="5" Grid.Column="2" Orientation="Horizontal" Margin="20,0,20,0" HorizontalAlignment="Center">
                            <Button x:Name="EntryEdit" TabIndex="5" Content="{x:Static p:Resources.vendor_edit}" Click="Button_Edit_Element_Click" Margin="20,0,5,0">
                                <Button.Style>
                                    <Style TargetType="{x:Type Button}">
                                        <Setter Property="Background" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColor, Mode=TwoWay}"/>
                                        <Setter Property="Foreground" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonFGColor, Mode=TwoWay}"/>
                                        <Setter Property="Template">
                                            <Setter.Value>
                                                <ControlTemplate TargetType="{x:Type Button}">
                                                    <Border Background="{TemplateBinding Background}" CornerRadius="0" Padding="20,5">
                                                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                    </Border>
                                                </ControlTemplate>
                                            </Setter.Value>
                                        </Setter>
                                        <Style.Triggers>
                                            <Trigger Property="IsMouseOver" Value="True">
                                                <Setter Property="Background" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                                <Setter Property="Foreground" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonFGColorHover, Mode=TwoWay}"/>
                                            </Trigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                            </Button>
                            <Button x:Name="EntryDelete" TabIndex="5" Content="{x:Static p:Resources.vendor_delete}" Margin="0,0,20,0" Click="EntryDelete_Click">
                                <Button.Style>
                                    <Style TargetType="{x:Type Button}">
                                        <Setter Property="Background" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColor, Mode=TwoWay}"/>
                                        <Setter Property="Foreground" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonFGColor, Mode=TwoWay}"/>
                                        <Setter Property="Template">
                                            <Setter.Value>
                                                <ControlTemplate TargetType="{x:Type Button}">
                                                    <Border Background="{TemplateBinding Background}" CornerRadius="0" Padding="20,5">
                                                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                                    </Border>
                                                </ControlTemplate>
                                            </Setter.Value>
                                        </Setter>
                                        <Style.Triggers>
                                            <Trigger Property="IsMouseOver" Value="True">
                                                <Setter Property="Background" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}"/>
                                                <Setter Property="Foreground" Value="{Binding Source={x:Static p:Settings.Default}, Path=ButtonFGColorHover, Mode=TwoWay}"/>
                                            </Trigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                            </Button>
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Grid.Row="2" Grid.Column="3" Margin="5,0" VerticalAlignment="Bottom">
                        <TextBlock x:Name="orderslbl" Text="{x:Static p:Resources.vendor_TotalOrders}" HorizontalAlignment="Center" FontSize="20"/>
                        <TextBox x:Name="Orders" MinWidth="20" Width="Auto" Text="0" IsTabStop="False" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" IsEnabled="False" FontSize="20"/>
                    </StackPanel>
                    <StackPanel Grid.Row="2" Grid.Column="4" Margin="5,0" VerticalAlignment="Bottom">
                        <TextBlock x:Name="orderstotlbl" Text="{x:Static p:Resources.vendor_SumOrders}" HorizontalAlignment="Center" FontSize="20"/>
                        <TextBox x:Name="sumOrders" MinWidth="40" Width="Auto" Text="0" IsTabStop="False" IsEnabled="False" FontSize="20" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
                    </StackPanel>
                    <DataGrid Grid.Row="6" Grid.Column="0" Grid.ColumnSpan="5" 
                          x:Name="ViewEntries" Height="300" MinHeight="100"
                          Width="Auto" MinWidth="600" MaxColumnWidth="200"
                          Margin="0,5,0,0" IsReadOnly="True"  VerticalAlignment="Top"
                          Background="{Binding Source={x:Static p:Settings.Default}, Path=innerBGColor, Mode=TwoWay}"
                          AlternatingRowBackground="#FFBFBFBF"  AlternationCount="-1" AutoGenerateColumns="False"
                          TabIndex="4" SelectionChanged="ViewEntries_SelectionChanged"
                          SelectedItem="{Binding txtbox, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" FontSize="16">
                        <DataGrid.Columns>
                            <DataGridTextColumn x:Name="IdColumn" Binding="{Binding Id}" Header="#" Width="Auto"/>
                            <DataGridTextColumn x:Name="DateColumn" Binding="{Binding Date}" Header="Order Date" Width="Auto"/>
                            <DataGridTextColumn x:Name="ReferenceColumn" Binding="{Binding Reference}" Header="Order Ref." Width="Auto"/>
                            <DataGridTextColumn x:Name="NameColumn" Binding="{Binding Name}" Header="Company" Width="Auto"/>
                            <DataGridTextColumn x:Name="VIDColumn" Binding="{Binding VID}" Header="Company ID" Width="Auto"/>
                            <DataGridTextColumn x:Name="DescriptionColumn" Binding="{Binding Description}" Header="Product Desc." Width="Auto"/>
                            <DataGridTextColumn x:Name="PIDColumn" Binding="{Binding PID}" Header="Product Id (SKU)" Width="Auto"/>
                            <DataGridTextColumn x:Name="QuantityColumn" Binding="{Binding Quantity}" Header="Quantity" Width="Auto"/>
                            <DataGridTextColumn x:Name="PriceColumn" Binding="{Binding Price}" Header="Price ($)" Width="Auto"/>
                            <DataGridTextColumn x:Name="DiscountColumn" Binding="{Binding Discount}" Header="Discount ($)" Width="Auto"/>
                            <DataGridTextColumn x:Name="ShippingColumn" Binding="{Binding Shipping}" Header="Shipping ($)" Width="Auto"/>
                            <DataGridTextColumn x:Name="TotalColumn" Binding="{Binding Total}" Header="Total Cost ($)" Width="Auto"/>
                        </DataGrid.Columns>
                    </DataGrid>
                    <Grid Grid.Row="6" Grid.RowSpan="1" Grid.ColumnSpan="5" x:Name="EditElementInputBox" Visibility="Collapsed">
                        <Grid Background="Black" Opacity="0.6"/>
                        <Border
                            MinWidth="250"
                            Background="{Binding Source={x:Static p:Settings.Default}, Path=innerBGColor, Mode=TwoWay}"
                            BorderBrush="Black" 
                            BorderThickness="2" 
                            CornerRadius="5,5,5,5" 
                            HorizontalAlignment="Center" 
                            VerticalAlignment="Center">
                            <StackPanel >
                                <TextBlock Margin="20,10,0,0" Text="{x:Static p:Resources.EditEntry}" FontWeight="Bold" />
                                <ScrollViewer MaxHeight="220">
                                    <Grid>
                                        <Grid.RowDefinitions>
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                            <RowDefinition />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                            <ColumnDefinition/>
                                        </Grid.ColumnDefinitions>
                                        <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryID}" Grid.Row="0" Grid.Column="0" HorizontalAlignment="Right"/>
                                        <TextBox x:Name="EditIdBox"  Margin="5,5,0,5" Width="200" Grid.Row="0" Grid.Column="1" IsEnabled="False"/>
                                        <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryDate}" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Right"/>
                                        <TextBox x:Name="EditDateBox" Margin="5,5,0,5" Width="200" Grid.Row="1" Grid.Column="1" ToolTip="{x:Static p:Resources.TooltipDateFormat}"/>
                                        <TextBlock Grid.Row="1" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                        <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryReference}" Grid.Row="2" Grid.Column="0" HorizontalAlignment="Right"/>
                                        <TextBox x:Name="EditReferenceBox" Margin="5,5,0,5" Width="200" Grid.Row="2" Grid.Column="1"/>
                                        <TextBlock Grid.Row="2" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                        <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryName}" Grid.Row="3" Grid.Column="0" HorizontalAlignment="Right"/>
                                        <ComboBox x:Name="EditNameBox" Margin="5,5,0,5" Width="200" Grid.Row="3" Grid.Column="1" LostFocus="EditNameBox_LostFocus" />
                                        <TextBlock Grid.Row="3" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                        <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryVID}" Grid.Row="4" Grid.Column="0" HorizontalAlignment="Right" IsEnabled="False"/>
                                        <TextBox x:Name="EditVIDBox" Margin="5,5,0,5" Width="200" Grid.Row="4" Grid.Column="1" IsEnabled="False"/>
                                        <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryProductDescription}" Grid.Row="5" Grid.Column="0" HorizontalAlignment="Right"/>
                                        <TextBox x:Name="EditDescriptionBox" Margin="5,5,0,5" Width="200" Grid.Row="5" Grid.Column="1"/>
                                        <TextBlock Grid.Row="5" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                        <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryPID}" Grid.Row="6" Grid.Column="0" HorizontalAlignment="Right"/>
                                        <TextBox x:Name="EditPIDBox" Margin="5,5,0,5" Width="200" Grid.Row="6" Grid.Column="1"/>
                                        <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryQuantity}" Grid.Row="7" Grid.Column="0" HorizontalAlignment="Right"/>
                                        <TextBox x:Name="EditQuantityBox" Margin="5,5,0,5" Width="200" Grid.Row="7" Grid.Column="1" PreviewTextInput="EditBox_PreviewTextInput" TextChanged="EditBox_TextChanged" />
                                        <TextBlock Grid.Row="7" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                        <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryPrice}" Grid.Row="8" Grid.Column="0" HorizontalAlignment="Right"/>
                                        <TextBox x:Name="EditPriceBox" Margin="5,5,0,5" Width="200" Grid.Row="8" Grid.Column="1" PreviewTextInput="EditBox_PreviewTextInput" TextChanged="EditBox_TextChanged" />
                                        <TextBlock Grid.Row="8" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                        <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryDiscount}" Grid.Row="9" Grid.Column="0" HorizontalAlignment="Right"/>
                                        <TextBox x:Name="EditDiscountBox" Margin="5,5,0,5" Width="200" Grid.Row="9" Grid.Column="1" PreviewTextInput="EditBox_PreviewTextInput" TextChanged="EditBox_TextChanged" />
                                        <TextBlock Grid.Row="9" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                        <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryShipping}" Grid.Row="10" Grid.Column="0" HorizontalAlignment="Right"/>
                                        <TextBox x:Name="EditShippingBox" Margin="5,5,0,5" Width="200" Grid.Row="10" Grid.Column="1" PreviewTextInput="EditBox_PreviewTextInput" TextChanged="EditBox_TextChanged"/>
                                        <TextBlock Grid.Row="10" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                        <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryTotal}" Grid.Row="11" Grid.Column="0" HorizontalAlignment="Right" IsEnabled="False"/>
                                        <TextBox x:Name="EditTotalBox" Margin="5,5,0,5" Width="200" Grid.Row="11" Grid.Column="1"/>
                                    </Grid>
                                </ScrollViewer>

                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                    <Button x:Name="EditCancelButton" Margin="5" Padding="10,5" Content="{x:Static p:Resources.popup_Cancel}" Background="{x:Null}" Click="ButtonEdit_Cancel" IsCancel="True" />
                                    <Button x:Name="EditUpdateButton" Margin="5" Padding="10,5" Content="{x:Static p:Resources.popup_Submit}" Background="{x:Null}" Click="ButtonEdit_Update" IsDefault="True" />
                                </StackPanel>
                            </StackPanel>
                        </Border>
                    </Grid>
                </Grid>
            </Border>
        </TabItem>
    </TabControl>
</UserControl>