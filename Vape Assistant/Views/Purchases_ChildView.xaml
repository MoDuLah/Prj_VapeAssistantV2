                                <TextBox Grid.Column="7" Grid.Row="2" TextChanged="Change_Total_Cost" PreviewTextInput="EditBox_PreviewTextInput" GotFocus="DoubleDigit_GotFocus" LostFocus="DoubleDigit_LostFocus" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" x:Name="total2"    Text="0.0" SelectionBrush="{Binding Source={x:Static p:Settings.Default}, Path=ButtonBGColorHover, Mode=TwoWay}" MaxLength="6"/>

