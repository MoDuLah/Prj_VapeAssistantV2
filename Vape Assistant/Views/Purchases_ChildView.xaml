                                    <TextBox x:Name="EditIdBox"  Margin="5,5,0,5" Width="200" Grid.Row="0" Grid.Column="1" IsEnabled="False"/>
                                    <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryDate}" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Right"/>
                                    <TextBox x:Name="EditDateBox" Margin="5,5,0,5" Width="200" Grid.Row="1" Grid.Column="1" ToolTip="{x:Static p:Resources.TooltipDateFormat}"/>
                                    <TextBlock Grid.Row="1" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                    <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryReference}" Grid.Row="2" Grid.Column="0" HorizontalAlignment="Right"/>
                                    <TextBox x:Name="EditReferenceBox" Margin="5,5,0,5" Width="200" Grid.Row="2" Grid.Column="1"/>
                                    <TextBlock Grid.Row="2" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                    <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryName}" Grid.Row="3" Grid.Column="0" HorizontalAlignment="Right"/>
                                    <ComboBox x:Name="EditNameBox" Margin="5,5,0,5" Width="200" Grid.Row="3" Grid.Column="1" LostFocus="EditNameBox_LostFocus" />
                                    <TextBlock Grid.Row="3" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                    <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryVID}" Grid.Row="4" Grid.Column="0" HorizontalAlignment="Right" IsEnabled="False"/>
                                    <TextBox x:Name="EditVIDBox" Margin="5,5,0,5" Width="200" Grid.Row="4" Grid.Column="1" IsEnabled="False"/>
                                    <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryProductDescription}" Grid.Row="5" Grid.Column="0" HorizontalAlignment="Right"/>
                                    <TextBox x:Name="EditDescriptionBox" Margin="5,5,0,5" Width="200" Grid.Row="5" Grid.Column="1"/>
                                    <TextBlock Grid.Row="5" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                    <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryPID}" Grid.Row="6" Grid.Column="0" HorizontalAlignment="Right"/>
                                    <TextBox x:Name="EditPIDBox" Margin="5,5,0,5" Width="200" Grid.Row="6" Grid.Column="1"/>
                                    <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryQuantity}" Grid.Row="7" Grid.Column="0" HorizontalAlignment="Right"/>
                                    <TextBox x:Name="EditQuantityBox" Margin="5,5,0,5" Width="200" Grid.Row="7" Grid.Column="1" PreviewTextInput="EditBox_PreviewTextInput" TextChanged="EditBox_TextChanged" />
                                    <TextBlock Grid.Row="7" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                    <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryPrice}" Grid.Row="8" Grid.Column="0" HorizontalAlignment="Right"/>
                                    <TextBox x:Name="EditPriceBox" Margin="5,5,0,5" Width="200" Grid.Row="8" Grid.Column="1" PreviewTextInput="EditBox_PreviewTextInput" TextChanged="EditBox_TextChanged" />
                                    <TextBlock Grid.Row="8" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                    <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryDiscount}" Grid.Row="9" Grid.Column="0" HorizontalAlignment="Right"/>
                                    <TextBox x:Name="EditDiscountBox" Margin="5,5,0,5" Width="200" Grid.Row="9" Grid.Column="1" PreviewTextInput="EditBox_PreviewTextInput" TextChanged="EditBox_TextChanged" />
                                    <TextBlock Grid.Row="9" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                    <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryShipping}" Grid.Row="10" Grid.Column="0" HorizontalAlignment="Right"/>
                                    <TextBox x:Name="EditShippingBox" Margin="5,5,0,5" Width="200" Grid.Row="10" Grid.Column="1" PreviewTextInput="EditBox_PreviewTextInput" TextChanged="EditBox_TextChanged"/>
                                    <TextBlock Grid.Row="10" Grid.Column="2" Text="*" Foreground="Red" Margin="0,5,20,0" VerticalAlignment="Center"/>
                                    <TextBlock Margin="5,5,0,5" Text="{x:Static p:Resources.EntryTotal}" Grid.Row="11" Grid.Column="0" HorizontalAlignment="Right" IsEnabled="False"/>
                                    <TextBox x:Name="EditTotalBox" Margin="5,5,0,5" Width="200" Grid.Row="11" Grid.Column="1"/>
                                </Grid>
                            </ScrollViewer>

                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                <Button x:Name="EditCancelButton" Margin="5" Padding="10,5" Content="{x:Static p:Resources.popup_Cancel}" Background="{x:Null}" Click="ButtonEdit_Cancel" IsCancel="True" />
                                <Button x:Name="EditUpdateButton" Margin="5" Padding="10,5" Content="{x:Static p:Resources.popup_Submit}" Background="{x:Null}" Click="ButtonEdit_Update" IsDefault="True" />
                            </StackPanel>
                        </StackPanel>
                    </Border>
                </Grid>
            </Grid>
        </TabItem>
    </TabControl>
</UserControl>